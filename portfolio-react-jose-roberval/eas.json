{
  "cli": {
    "version": ">= 3.13.0",
    "appVersionSource": "appConfig"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "simulator": false
      }
    }
  },
  "submit": {
    "development": {
      "android": {},
      "ios": {}
    },
    "preview": {
      "android": {},
      "ios": {}
    },
    "production": {
      "android": {},
      "ios": {}
    }
  }
}
